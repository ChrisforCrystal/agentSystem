<struts>
	<package name="logs" namespace="/" extends="global">
		<!-- 设置Action -->
		<action name="mylogs" class="logAction" method="logs">
			<interceptor-ref name="adminIntegerceptor"></interceptor-ref> 
			<result name="success">/WEB-INF/pages/logs.jsp</result>
		</action>
	</package>
</struts>  
