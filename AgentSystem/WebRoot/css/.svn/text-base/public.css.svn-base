@charset "utf-8";
/* CSS Document */
/*此CSS为全局CSS 影响所有页面 为layout*/
.none{
	display:none;
}
body{
	font-size:12px;
	font-family:"微软雅黑",Verdana, Geneva, sans-serif;
	margin:0px;
	padding:0px;
	/*background: url("/imgs/bgd.jpg");*/
	background:#A0DAF4;
}
ul,li,p,span,div,form,input,textarea,strong,b,i,em,h1,h2,h3,h4,h5,h6,select{
	padding:0px;
	margin:0px;
	/*text-shadow: 0 1px 0 #FFF;*/
}
ul{
	list-style-type:none;
}
a{
	text-decoration:none;
	color:#fff;
}
img{
	border:0px;
}
table{
	border-collapse:collapse;
	width:98%;margin:0 auto;}

	
td{
	padding:5px;

	border:1px solid #cccccc;
}
td,th{
	font-size:12px;
}
h1{
	display:block; 
}
.addAppTitle h2{
	padding-left:12px;
}
.clear{
	height:0px;
	line-height:1;
	font-size:0px;
	overflow:hidden;
	zoom:1;
	clear:both;
}
.head{
	height:120px;
	overflow:hidden;
	zoom:1; 
	color:#3f3f3f;
    line-height: 20px;
    text-shadow: 0 1px 0 #FFF;
 	background:url(../imgs/top_1px.gif) repeat-x;
}
.head ul{
	height:120px;
	width:1000px;
	margin:0 auto;
	background:url(../imgs/top_bg.gif) no-repeat;
}
.head  li ul{
	height:auto;
	width:auto;
	float:right;
	background:none; 
}
 
.head li{
	float:left;
	width:49%;
	display:block;
	margin-top:10px;
	}

.headfunc{
	text-align: right;
	word-spacing:8px;
	letter-spacing: 1px;
	font-weight: normal;
	font-size:12px;
	float:right;
}
.headfunc li{
	width:100%;
}
.headfunc a{cursor: pointer;
	color:#3f3f3f;
    text-shadow: 0 1px 0 #FFF;

}
.modifyPasswordContent{
	padding:10px;
}
.modifyPasswordContent li{
	padding:5px;
	text-align: left;
}
.modifyPasswordContent .btnli{text-align: right;padding-right:30px;}
.modifyPasswordContent li span{
	color:#4696E5;
}
.modifydiv{
		position:absolute;
		right:10px;
		top:40px;
		display:none;
		background: #fff url(../imgs/dialog_bg.gif) repeat-x left bottom;
		border:2px solid #37ADDF;
		border-radius:5px;
		-webkit-border-radius:5px;
		-moz-border-radius:5px;
		-o-border-radius:5px;
		-ms-border-radius:5px;
		text-align: center;
		box-shadow: 0 1px 0 #FFF;
		filter:progid:DXImageTransform.Microsoft.Shadow(color=#909090,direction=120,strength=4);/*ie*/
		-moz-box-shadow: 2px 2px 10px #909090;/*firefox*/
		-webkit-box-shadow: 2px 2px 10px #909090;/*safari或chrome*/
		box-shadow:2px 2px 10px #909090;/*opera或ie9*/
		z-index:100;
}

.modifTop{
	height:17px;
	line-height:17px;
	background:#fff url(../imgs/diaolog_top_1px.png) repeat-x left bottom;
	border:1px solid #37ADDF;
	border-radius: 5px 5px 0px 0px;
	-webkit-border-radius:5px 5px 0px 0px;
	-moz-border-radius:5px 5px 0px 0px;
	-o-border-radius:5px 5px 0px 0px;
	-ms-border-radius:5px 5px 0px 0px;
	width:366px;
}
.mbxnav{
	height:50px;
	width:978px;
	margin: 0 auto;
	overflow:hidden;
	line-height:50px;
    padding-left:12px
}
	.mbxnav a{
	color:#3f3f3f;
    text-shadow: 0 1px 0 #FFF;
	
	}
.container{
	width:980px;
	margin: 0 auto;
	overflow:hidden;
	zoom:1;
	background:#fff;
	/*background:url("/imgs/gradient.png") repeat-x scroll 0 0 #D2D1D0;*/
	/*background: url("/imgs/content_bg_slice-2e234d4b4095b97a7f901ddc3194d517.png") repeat-x scroll 0 0 #D2D1D0;*/
    height: 100%;
    line-height: 18px;
    min-height: 350px;
    padding: 20px 0 30px;
	
  	border-radius:0px 0px 5px 5px;
  	-webkit-border-radius:0px 0px 5px 5px;
  	-moz-border-radius:0px 0px 5px 5px;
  	-o-border-radius:0px 0px 5px 5px;
  	-ms-border-radius:0px 0px 5px 5px;
  	text-align: center;
  	box-shadow: 0 1px 0 #FFF;
  	filter:progid:DXImageTransform.Microsoft.Shadow(color=#909090,direction=120,strength=4);/*ie*/
	-moz-box-shadow: 2px 2px 10px #909090;/*firefox*/
	-webkit-box-shadow: 2px 2px 10px #909090;/*safari或chrome*/
	box-shadow:2px 2px 10px #909090;/*opera或ie9*/
}
/*footer*/
.foot{
	margin-top:8px;
	text-align:center;
	color:#3f3f3f;
	font-size:12px;
	text-shadow: 0 1px 0 #FFF;
}
.pagination {
  height: 36px;
  margin: 18px 0;
}

.pagination ul {
  display: inline-block;
  *display: inline;
  margin-bottom: 0;
  margin-left: 0;
  -webkit-border-radius: 3px;
     -moz-border-radius: 3px;
          border-radius: 3px;
  *zoom: 1;
  -webkit-box-shadow: 0 1px 2px rgba(0, 0, 0, 0.05);
     -moz-box-shadow: 0 1px 2px rgba(0, 0, 0, 0.05);
          box-shadow: 0 1px 2px rgba(0, 0, 0, 0.05);
}

.pagination li {
  display: inline;
}

.pagination a {
  float: left;
  padding: 0 14px;
  line-height: 34px;
  text-decoration: none;
  border: 1px solid #ddd;
  border-left-width: 0;
}

.pagination a:hover,
.pagination .active a {
  background-color: #f5f5f5;
}

.pagination .active a {
  color: #999999;
  cursor: default;
}

.pagination .disabled span,
.pagination .disabled a,
.pagination .disabled a:hover {
  color: #999999;
  cursor: default;
  background-color: transparent;
}

.pagination li:first-child a {
  border-left-width: 1px;
  -webkit-border-radius: 3px 0 0 3px;
     -moz-border-radius: 3px 0 0 3px;
          border-radius: 3px 0 0 3px;
}

.pagination li:last-child a {
  -webkit-border-radius: 0 3px 3px 0;
     -moz-border-radius: 0 3px 3px 0;
          border-radius: 0 3px 3px 0;
}

.pagination-centered {
  text-align: center;
}

.pagination-right {
  text-align: right;
}
/* menu */
#menu{height:32px;background:url(../imgs/top_1px.gif) repeat-x 0px  -120px; }

#menu ul{width:778px;height:32px;margin:0 auto;}
#menu .m_li{float:left;width:114px;line-height:32px;text-align:center;margin:0px -2px;}
#menu .m_li a{display:block;color:#FFFFFF;width:114px;}
#menu .m_line{float:left;width:1px;height:32px;line-height:32px;/*ff下有效(图片垂直居中)*/}
#menu .m_line img{margin-top:expression(( 32 - this.height ) / 2);/*ie下有效(图片垂直居中)*/margin-top:6px;}
#menu .m_li_a{float:left;width:98px;text-align:center;;font-weight:bold;position:relative;height:32px;line-height:32px;}
#menu .m_li_a a{
		display:block;
		color:#0D4C6B;
		width:114px;
		height:36px;
		line-height:36px;
		font-weight:bold;
		background:url(../imgs/menu_bg.png) no-repeat;
		margin-top:-4px;
}
/* subbox */
.subbox{height:32px;background:url(../imgs/top_1px.gif) repeat-x 0px  -162px;}
.subbox .smenu{width:804px;margin:0px auto;height:32px;}
.subbox li{line-height:32px;height:32px;}
.subbox .s_li{display:none;}
.subbox .s_li_a{display:block;}
.smenu a{
	color:#0D4C6B;
	text-decoration: none;
	padding:5px 15px;
	background:url(../imgs/line1.white.gif) no-repeat right center;
}
.subbox .smenu a:HOVER {
	color:#ff0000;
	text-decoration: none;
}


